<nav class="navbar navbar-expand-lg navbar-dark bg-dark">

  <% if user_signed_in? || organizator_signed_in? %>
      <p class="navbar-brand">YoLoCubro.com |</p>

        <% if current_user.present? %>
          <p class="pbar">Bienvenido <%= current_user.name %></p>
        <% else %>
          <p class="pbar">Bienvenido <%= current_organizator.name %></p>
        <% end %>
  <%else %>
      <a class="navbar-brand" href="<%= root_path %>">YoLoCubro.com</a>
  <%end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav ml-auto">

      <% if user_signed_in? %>
      <li class="nav-item">
      </li>
        <li class="nav-item">
          <%= link_to "Salir", destroy_user_session_path, method: :delete, class: "nav-link" %></li>

      <% end %>

    <% if organizator_signed_in? %>
      <li class="nav-item">
      </li>
        <li class="nav-item">
            <%= link_to "Salir", destroy_organizator_session_path, method: :delete, class: "nav-link" %></li>
        <%end%>
    </ul>
  </div>
</nav>
